Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ExpandHodgeDual", "[", 
    RowBox[{"expr_", ",", 
     RowBox[{"Coframe", "[", 
      RowBox[{"mani_", "?", "ManifoldQ"}], "]"}], ",", "met_"}], "]"}], ":=", 
   
   RowBox[{"expr", "/.", 
    RowBox[{
     RowBox[{"HoldPattern", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Hodge", "[", "met", "]"}], "[", 
        RowBox[{"form", ":", 
