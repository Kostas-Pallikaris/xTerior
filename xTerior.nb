 RowBox[{
  RowBox[{
   RowBox[{"ExpandHodgeDual", "[", 
    RowBox[{"expr_", ",", "\[Theta]_"}], "]"}], ":=", 
   RowBox[{"expr", "/.", 
    RowBox[{
     RowBox[{"HoldPattern", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Hodge", "[", "met_", "]"}], "[", 
        RowBox[{"form", ":", 
         RowBox[{"Wedge", "[", 
          RowBox[{
           RowBox[{"\[Theta]", "[", "_", "]"}], ".."}], "]"}]}], "]"}], "|", 
       RowBox[{"form", ":", 
        RowBox[{
         RowBox[{"Hodge", "[", "met_", "]"}], "[", 
         RowBox[{"\[Theta]", "[", "_", "]"}], "]"}]}]}], "]"}], 
