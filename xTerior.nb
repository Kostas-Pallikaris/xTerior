          RowBox[{"1", "/", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"dim", "-", "n"}], ")"}], "!"}]}], 
          RowBox[{
           RowBox[{"epsilon", "[", "met", "]"}], "@@", 
           RowBox[{"Join", "[", 
            RowBox[{"inds", ",", 
