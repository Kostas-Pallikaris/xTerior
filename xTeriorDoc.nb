Cell[TextData[{
 StyleBox["FormVarD\t\t ", "Input",
  FontWeight->"Plain",
  FontColor->RGBColor[0, 0, 1]],
 StyleBox["Variational derivative of a Lagrangian constructed with \
differential forms",
  FontColor->GrayLevel[0]]
}], "FramedBox",
